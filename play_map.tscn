[sub_resource type="GDScript" id=1]
script/source = "extends Node2D


# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"

var resource_names = [ \"power\", \"oxygen\", \"hull\", \"doorpower\" ]

var current_power = 0
var has_power_updated = true

var current_oxygen = 100
var has_oxygen_updated = true

var current_hull = 100
var has_speed_updated = true

var current_door_power = 0
var has_door_power_updated = true

# Called when the node enters the scene tree for the first time.
func _ready():
	for object in $objects.get_children():
		if object.is_in_group(\"give_resource\") :
			object.connect(\"give_resource\", self, \"_on_resource_given\")
		if object.is_in_group(\"take_resource\") :
			object.connect(\"take_resource\", self, \"_on_resource_taken\")
	
	for resource in resource_names :
		_update_resource_bars(resource)

func _set_resource(name, amount):
	match name:
		\"power\": current_power = amount
		\"oxygen\": current_oxygen = amount
		\"hull\": current_hull = amount
		\"doorpower\": current_door_power = amount
		
func _get_resource(name):
	match name:
		\"power\": return current_power
		\"oxygen\": return current_oxygen
		\"speed\": return current_hull
		\"doorpower\": return current_door_power
	
func _alter_resource(name, amount):
	var is_taking = amount < 0
	var is_giving = amount > 0
	
	var resource_amount = _get_resource(name)
	if is_taking :
		if resource_amount >= amount :
			resource_amount += amount
			_set_resource(name, resource_amount)
			_update_resource_bars(name)
			return true
		else:
			return false
	else :
		resource_amount += amount
		_set_resource(name, resource_amount)
		_update_resource_bars(name)
				
				
func _update_resource_bars(name):
	for bar in $Control.get_children():
		if bar.is_in_group(name + \"bar\"):
			var resource_amount = _get_resource(name)
			bar.animate_to_value(resource_amount)

func _on_resource_given(name, amount):
	_alter_resource(name, amount)

func _on_resource_taken(requester, name, amount):
	var was_successful = _alter_resource(name, -amount)
	if was_successful :
		requester.give_resource()

func _on_enemy_timer_timeout():
	var enemies = randi()%10+1
	for object in $objects.get_children():
		if object.is_in_group(\"radar\") :
			object.set_display(enemies)
"

